# Frontend (required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Backend (required for full functionality)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Bright Data (optional - falls back to direct fetch)
BRIGHTDATA_PROXY_HOST=
BRIGHTDATA_PROXY_PORT=22225
BRIGHTDATA_USERNAME=
BRIGHTDATA_PASSWORD=

# Gemini (optional - returns placeholder if missing)
GEMINI_API_KEY=

# Acontext (optional - falls back to in-memory)
ACONTEXT_API_URL=https://api.acontext.ai/v1
ACONTEXT_API_KEY=

# ActionBook (optional - fallback scraper)
ACTIONBOOK_API_URL=https://api.actionbook.io/v1
ACTIONBOOK_API_KEY=

# Server
PORT=3001
